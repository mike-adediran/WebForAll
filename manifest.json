{
    "manifest_version": 3,
    "name": "E-commerce Accessibility MVP",
    "version": "1.0",
    "description": "A basic accessibility extension for e-commerce websites",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage"]
  }
  